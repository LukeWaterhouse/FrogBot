    "StopLossPct": -5,
    "TransactionCostPct": 0.1,
    "Universe": [
      "SPY",
      "QQQ",
      "AAPL",
      "MSFT",
      "GOOGL",
      "AMZN",
      "NVDA",
      "META",
      "TSLA",
      "JPM"
    ]
  }
}
